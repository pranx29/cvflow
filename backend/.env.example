# Application
APP_ENV=development  
APP_HOST=127.0.0.1
APP_PORT=8000
DEBUG=True
SECRET_KEY=change_this_to_a_secure_random_string

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=your_aws_region

# S3 Configuration
S3_BUCKET_NAME=your_s3_bucket_name
S3_BUCKET_PATH=your_s3_bucket_path
S3_PRESIGNED_URL_EXPIRY=3600  

# AWS Lambda Configuration
LAMBDA_FUNCTION_ARN=your_lambda_function_arn
EVENTBRIDGE_SCHEDULER_ROLE_ARN=your_eventbridge_scheduler_role_arn

# Google Sheets
GOOGLE_SHEETS_CREDENTIALS_FILE=path_to_your_google_sheets_credentials_file
GOOGLE_SHEET_ID=your_google_sheet_id

# OpenRouter API
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_LLM_API_MODEL=your_openrouter_llm_api_model
OPENROUTER_API_URL=your_openrouter_api_url

# Webhook
WEBHOOK_URL=your_webhook_url
CANDIDATE_EMAIL=your_candidate_email

# Email Scheduling
EMAIL_DELAY_HOURS=24  